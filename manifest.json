{
  "manifest_version": 3,
  "name": "AI Ad Blocker",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ]
}